{
    "production": {
        "app_function": "terracotta.app.app",
        "aws_region": "eu-central-1",
        "profile_name": "default",
        "project_name": "terracotta",
        "runtime": "python3.6",
        "s3_bucket": "zappa-teracotta",
        "aws_environment_variables": {
            "TC_DRIVER_PATH": "{path to remote database}",
            "TC_DRIVER_PROVIDER": "sqlite-remote",
            "TC_UPSAMPLING_METHOD": "cubic",
            "TC_XRAY_PROFILER": "true"
        },
        "callbacks": {
            "zip": "zappa_version_callback.inject_version"
        },
        "exclude": [
            "*.gz",
            "*.rar",
            "boto3*",
            "botocore*",
            "awscli*",
            ".mypy_cache",
            ".pytest_cache",
            ".eggs"
        ],
        "cache_cluster_enabled": true,
        "cache_cluster_size": 0.5,
        "cache_cluster_ttl": 3600,
        "debug": false,
        "log_level": "WARNING",
        "xray_tracing": true
    }
}